		"prettier": "3.5.3",
		"tsdown": "0.8.0-beta.4",
		"tsx": "4.19.4",
		"typescript": "5.8.3",
		"typescript-eslint": "8.31.1"
	},
	"browserslist": {
		"src": [
			"node >= 18"
		],
		"scripts": [
			"node >= 22"
		]
	}
